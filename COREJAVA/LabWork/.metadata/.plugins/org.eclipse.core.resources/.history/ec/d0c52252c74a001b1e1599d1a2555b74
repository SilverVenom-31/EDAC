package com.app.utils;

import java.util.ArrayList;
import java.util.List;
import static java.time.LocalDate.parse;
import com.app.core.Student;
import com.app.core.Subject;

public class CollectionUtils {

	public static List<Student> populateList(){
		
		ArrayList<Student> list = new ArrayList<>();
		
		list.add(new Student("100", "Alpha",parse("2020-03-21") ,Subject.CLOUD, 6.75));
		list.add(new Student("103", "Bravo",parse("2020-02-24") ,Subject.DBT, 9.7));
		list.add(new Student("102", "Alpha",parse("2020-01-22") ,Subject.ML, 8.8));
		list.add(new Student("101", "Alpha",parse("2020-05-25") ,Subject.JAVA, 7.75));
		
		return list;
	}
	
}
